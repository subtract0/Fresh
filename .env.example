# OpenAI Configuration
OPENAI_API_KEY="your_openai_api_key_here"
ASSISTANT_ID='your_assistant_id_here'
OPENAI_ASSISTANT_ID='your_assistant_id_here'
VECTOR_STORE_ID='your_vector_store_id_here'
DAILY_OUT_TOKENS_CAP=10000

# Firebase/Google Cloud Configuration
# CRITICAL: This file contains sensitive API keys and must never be committed to Git

# Google Cloud API Key for Firebase Web SDK
GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key_here

# Firebase Configuration
FIREBASE_PROJECT_ID=your_project_id_here
FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your_project_id.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
FIREBASE_APP_ID=your_app_id_here

# Android Development Configuration
# Debug Keystore Information (for Google OAuth Client ID setup)
ANDROID_DEBUG_KEYSTORE_PATH=./debug.keystore
ANDROID_DEBUG_KEYSTORE_ALIAS=androiddebugkey
ANDROID_DEBUG_KEYSTORE_PASSWORD=android
ANDROID_DEBUG_KEY_PASSWORD=android
ANDROID_DEBUG_SHA1_FINGERPRINT=your_sha1_fingerprint_here
ANDROID_DEBUG_SHA256_FINGERPRINT=your_sha256_fingerprint_here

# OAuth 2.0 Client IDs
ANDROID_OAUTH_CLIENT_ID=your_oauth_client_id_here

# Environment
NODE_ENV=production
REF_API_KEY=your_ref_api_key_here
